{
    "name": "copy/paste-history",
    "displayName": "Copy/Paste History",
    "description": "Keep a history of your copied items and re-paste if needed",
    "version": "0.0.1",
    "publisher": "Anjali",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.copy",
        "onCommand:extension.cut",
        "onCommand:extension.paste",
        "onCommand:extension.addToHistory",
        "onCommand:extension.removeFromHistory"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.copy",
                "title": "Copy to Clipboard"
            },
            {
                "command": "extension.cut",
                "title": "Cut to Clipboard"
            },
            {
                "command": "extension.paste",
                "title": "Paste"
            },
            {
                "command": "extension.pasteFromClipBoard",
                "title": "Paste from Clipboard"
            },
            {
                "command": "extension.removeFromClipboard",
                "title": "Remove from Clipboard"
            },
            {
                "command": "extension.editClipboard",
                "title": "Edit Clipboard"
            }
        ],
        "keybindings":[
            {
                "command": "extension.copy",
                "key": "ctrl+c"
            },
            {
                "command": "extension.cut",
                "key": "ctrl+x"
            },
            {
                "command": "extension.paste",
                "key": "ctrl+v"
            },
            {
                "command": "extension.pasteFromClipboard",
                "key": "ctrl+shift+v"
            },
            {
                "command": "extension.removeFromClipboard",
                "key": "ctrl+alt+v d"
            },
            {
                "command": "extension.editClipboard",
                "key": "ctrl+alt+v e"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}